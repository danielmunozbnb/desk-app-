{
  "id": "checkOut",
  "include": [
    "states.script"
  ],
  "outboundData": {
    "outboundEndPoints": [
      {
        "name": "checkOutBooking",
        "baseUrlType": "app",
        "url": "<% 'bookings/' + queryParams.bookingId %>",
        "httpMethod": "PATCH",
        "authType": "sso",
        "values": [
          {
            "outboundPath": "state",
            "value": "<% states.checkedOut %>"
          }
        ]
      }
    ]
  },
  "presentation": {
    "headerSize": "VPS_DEFAULT",
    "title": {
      "type": "title",
      "label": "Check-Out Booking"
    },
    "body": {
      "type": "section",
      "horizontal": false,
      "children": [
        {
          "type": "readOnlyText",
          "label": "Check-Out?",
          "value": "(show booking)"
        },
        {
          "type": "checkBox",
          "label": "Confirm",
          "required": true
        }
      ]
    }
  }
}